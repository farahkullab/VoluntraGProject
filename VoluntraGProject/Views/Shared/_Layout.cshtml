<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>Voluntra</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="Free Website Template" name="keywords">
    <meta content="Free Website Template" name="description">

    <!-- Favicon -->
    <link href="~/Helpz/img/favicon.ico" rel="icon">

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css2?family=Quicksand:wght@300;400;500;600;700&display=swap" rel="stylesheet">

    <!-- CSS Libraries -->
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="~/Helpz/lib/flaticon/font/flaticon.css" rel="stylesheet">
    <link href="~/Helpz/lib/animate/animate.min.css" rel="stylesheet">
    <link href="~/Helpz/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/Account+dash/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Account+dash/css/line-awesome.css" />
    <link rel="stylesheet" href="~/Account+dash/css/animate.min.css" />
    <link rel="stylesheet" href="~/Account+dash/css/owl.carousel.min.css" />
    <link rel="stylesheet" href="~/Account+dash/css/owl.theme.default.min.css" />
    <link rel="stylesheet" href="~/Account+dash/css/select2.min.css" />
    <link rel="stylesheet" href="~/Account+dash/css/fancybox.css" />
    <link rel="stylesheet" href="~/Account+dash/css/tooltipster.bundle.css" />
    <link rel="stylesheet" href="~/Account+dash/css/leaflet.css" />
    <link rel="stylesheet" href="~/Account+dash/css/style.css" />

    <!-- Template Stylesheet -->
    <link href="~/Helpz/css/style.css" rel="stylesheet">
</head>

<body>
    <!-- Top Bar Start -->
    <div class="top-bar d-none d-md-block">
        <div class="container-fluid">
        </div>
    </div>
    <!-- Top Bar End -->
    <!-- Nav Bar Start -->
    <div class="navbar navbar-expand-lg bg-dark navbar-dark">
        <div class="container-fluid">
            <a href="index.html" class="navbar-brand">Voluntra</a>
            <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#navbarCollapse">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse justify-content-between" id="navbarCollapse">
                <div class="navbar-nav ml-auto">
                    <a asp-controller="Home" asp-action="Index" class="nav-item nav-link active">Home</a>
                    <a asp-controller="Home" asp-action="About" class="nav-item nav-link">About</a>
                    <a asp-controller="Event" asp-action="Event" class="nav-item nav-link">Events</a>
                    <a asp-controller="Home" asp-action="Blog" class="nav-item nav-link">Blog</a>
                    <div class="nav-item dropdown">
                        <a href="#" class="nav-link dropdown-toggle" data-toggle="dropdown">Language</a>
                        <div class="dropdown-menu">
                            <a href="single.html" class="dropdown-item">English</a>
                            <a href="service.html" class="dropdown-item">Arabic</a>
                        </div>
                    </div>
                    <a href="/#Feedback" class="nav-item nav-link">Contact</a>
                </div>
            </div>
        </div>
    </div>
    <!-- Nav Bar End -->

    <div>
        <main role="main">

            @RenderBody()

        </main>
    </div>

    <!-- Footer Start -->
    <div class="footer">
        <div class="container">
            <div class="row">
                <div class="col-lg-3 col-md-6">
                    <div class="footer-contact">
                        <h2>Our Head Office</h2>
                        <p><i class="fa fa-map-marker-alt"></i>123 Street, New York, USA</p>
                        <p><i class="fa fa-phone-alt"></i>+012 345 67890</p>
                        <p><i class="fa fa-envelope"></i>info@example.com</p>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="footer-link">
                        <h2>Popular Links</h2>
                        <a asp-controller="Home" asp-action="About">About Us</a>
                        <a href="">Contact Us</a>
                        <a href="">Popular NGOs</a>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="footer-link">
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="footer-newsletter">
                        <h2>Quick Contact</h2>
                        <form>
                            <section class="quick-contact">
                                <input type="email" name="email" placeholder="Your Email*">
                                <textarea placeholder="Type your message*"></textarea>
                            <button class="btn btn-custom">Submit</button>
                            <label>Don't worry, we don't spam!</label>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <div class="container copyright">
            <div class="row">
                <div class="col-md-6">
                    <p>&copy; <a href="#">Your Site Name</a>, All Right Reserved.</p>
                </div>
                <div class="col-md-6">
                </div>
            </div>
        </div>
    </div>
    <!-- Footer End -->
    <!-- Back to top button -->
    <a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>

    <!-- Pre Loader -->
    <div id="loader" class="show">
        <div class="loader"></div>
    </div>

    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
    <script src="~/Helpz/lib/easing/easing.min.js"></script>
    <script src="~/Helpz/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/Helpz/lib/waypoints/waypoints.min.js"></script>
    <script src="~/Helpz/lib/counterup/counterup.min.js"></script>
    <script src="~/Helpz/lib/parallax/parallax.min.js"></script>

    <!-- Contact Javascript File -->
    <script src="~/Helpz/mail/jqBootstrapValidation.min.js"></script>
    <script src="~/Helpz/mail/contact.js"></script>

    <!-- Template Javascript -->
    <script src="~/Helpz/js/main.js"></script>
    <script src="~/Account+dash/js/jquery-3.7.1.min.js"></script>
    <script src="~/Account+dash/js/bootstrap.bundle.min.js"></script>
    <script src="~/Account+dash/js/select2.min.js"></script>
    <script src="~/Account+dash/js/owl.carousel.min.js"></script>
    <script src="~/Account+dash/js/waypoint.min.js"></script>
    <script src="~/Account+dash/js/jquery.counterup.min.js"></script>
    <script src="~/Account+dash/js/main.js"></script>

</body>
</html>